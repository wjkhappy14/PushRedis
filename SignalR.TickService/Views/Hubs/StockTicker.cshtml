@{
                /**/

                Layout = "~/Views/Home/App.cshtml";
}
<head>
    <link href="/Hubs/StockTicker/StockTicker.css?v=@DateTime.Now.Ticks" rel="stylesheet" />
    <script src="/Hubs/StockTicker/SignalR.StockTicker.js?v=@DateTime.Now.Ticks"></script>
    <script src="/Hubs/StockTicker/Subscribe.js?v=@DateTime.Now.Ticks"></script>
</head>

<script id="new-message-template" type="text/x-jquery-tmpl">
    <li class="message" id="m-${id}">
        <strong>${name}:</strong>
        {{html message}}
    </li>
</script>
<script id="new-user-template" type="text/x-jquery-tmpl">
    <li class="user" id="u-${name}">
        <img src="http://www.gravatar.com/avatar/${hash}?s=16&d=mm" class="gravatar" /> ${name}
    </li>
</script>

<div id="chat">
    <div id="chat-area">
        <ul id="messages"></ul>
        <ul id="users"></ul>
        <div class="clear">
        </div>
        <form id="send-message" action="#">
            <input type="text" id="new-message" value="How are you ?" />
            <input type="submit" id="send" value="发送" />
        </form>
    </div>
</div>
<div>
    <input type="button" id="open" value="开市" />
    <input type="button" id="close" value="休市" disabled="disabled" />
    <input type="button" id="reset" value="重置" />
    <div id="stockTicker">
        <div class="inner">
            <ul>
                <li class="loading">加载中...</li>
            </ul>
        </div>
    </div>
    <div id="stockTable">
        <table border="1">
            <thead>
                <tr>
                    <td>CommodityNo</td>
                    <td>ContractNo</td>
                    <td>CurrentTime</td>
                    <td>Time</td>
                    <td>TimeDiff</td>
                    <td>HighPrice</td>
                    <td>LastPrice</td>
                    <td>LastSize</td>
                    <td>LowPrice</td>
                    <td>NowClosePrice</td>
                    <td>ClosePrice</td>
                    <td>OpenPrice</td>
                    <td>PercentChange</td>
                    <td>PositionQty</td>
                    <td>PrePositionQty</td>
                    <td>PreSettlePrice</td>
                    <td>TotalQty</td>
                    <td>TotalVolume</td>
                    <td>Volume</td>
                    <td>AskPrice</td>
                    <td>AskSize</td>
                    <td>BidPrice</td>
                    <td>BidSize</td>
                </tr>
            </thead>
            <tbody>
                <tr class="loading"><td colspan="7">加载中...</td></tr>
            </tbody>
        </table>
    </div>

</div>
